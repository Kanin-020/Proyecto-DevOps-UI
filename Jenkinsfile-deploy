pipeline {
  agent any
  stages {
    stage('Construir') {
        steps {
        powershell 'npm install'
        powershell 'npm run build'
      }
    }
    stage('Desplegar'){
        steps {
          powershell '$location = (Get-Location)'
          powershell 'Write-Output ${location}/dist'
          powershell 'pscp -r ${location}/dist jorge@192.168.1.95:/var/www/devopshot'
        }
    }
  }
}

