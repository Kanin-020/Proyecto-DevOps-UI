pipeline {
  agent any
  stages {
    stage('Construir') {
        steps {
        powershell 'npm install'
        powershell 'npm run build'
      }
    }
    stage('Desplegar'){
        steps {
          def location = env.WORKSPACE
          powershell 'Write-Output ${location}'
          powershell 'pscp -r ${location} jorge@192.168.1.95:/var/www/devopshot'
        }
        }
    }
  }